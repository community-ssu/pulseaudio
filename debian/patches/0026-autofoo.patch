+	rm -f $(DESTDIR)$(libdir)/libpulsedsp.*
+	rm -f $(DESTDIR)$(modlibexecdir)/*.so
+# Nokia pulsecore hack
+	rm -rf $(DESTDIR)$(includedir)/pulsecore
